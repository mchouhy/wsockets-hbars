<h1 class="title">Listado de productos</h1>

<div class="products-wrapper">
{{#each products}}
<div class="product-container">
            <h2>Nombre:<br> {{this.title}}</h2>
            <p>Descripción:<br> {{this.description}}</p>
            <p>Precio:<br> €{{this.price}}</p>
</div> 
{{/each}}
</div>