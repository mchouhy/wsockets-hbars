<h1 class="title">Manejador de productos en tiempo real</h1>
<div id="productsRT" class="products-wrapper">
      {{#each products}}
      <div class="product-container">
            <h2>Nombre:<br> {{this.title}}</h2>
            <p>Descripción:<br> {{this.description}}</p>
            <p>Precio:<br> €{{this.price}}</p>
            <button>Eliminar producto</button>
      </div>
      {{/each}}
</div>
<form id="addProductForm" class="inputs-container" action="">
            <div>
                  <label for="title">Nombre:</label>
                  <input type="text" id="title">
            </div>
            <div>
                  <label for="title">Descripción:</label>
                  <input type="text" id="description">
            </div>
            <div>
                  <label for="title">Precio:</label>
                  <input type="number" id="price">
            </div>
            <div>
                  <label for="title">Thumbnails:</label>
                  <input type="text" id="thumbnails" value="[]" disabled>
            </div>
            <div>
                  <label for="title">Stock:</label>
                  <input type="number" id="stock">
            </div>
            <div>
                  <label for="title">Código:</label>
                  <input type="text" id="code">
            </div>
            <div>
                  <label for="title">Estado:</label>
                  <input value="true" type="text" id="status" disabled>
                  
            </div>
            <div>
                  <label for="title">Categoría:</label>
                  <input value="muebles" type="text" id="category" disabled>
            </div>
      <button type="submit">Agregar producto</button>
</form>

<script src="https://cdn.socket.io/4.7.4/socket.io.min.js"></script>
<script type="module" src="js/main.js"></script>